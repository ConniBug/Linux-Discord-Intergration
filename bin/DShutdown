#!/bin/bash
while test $# -gt 0; do
  case "$1" in
    -i|--info)
      echo "Linux Discord Intergration"
      echo "Maintainer - Conni!~#0920"
      echo "GitHub - https://github.com/ConniTheKiwi/Linux-Discord-Intergration"
      exit 0
      ;;
    *)
      break
      ;;
  esac
done

DWebhook "'$USER' Ran command - DShutdown $1 $2 $3 $4 $5"
